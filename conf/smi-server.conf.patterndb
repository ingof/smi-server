destination d_smi_normal { file("/var/log/smi-server.log"); };
destination d_smi_error { file("/var/log/smi-server_error.log"); };
# filter f_smi_normal { facility(local0); };
# filter f_smi_error { facility(local3); };
filter f_smi_normal { level(debug..warning); };
filter f_smi_error { level(err..emerg); };
log { source(src); filter(f_smi_normal); destination(d_smi_normal); };
log { source(src); filter(f_smi_error); destination(d_smi_error); };
